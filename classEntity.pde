abstract class Entity
{
    //members: data stored for a entity object
    int x; //x cords
    int y; //y cords
    int entitySize;

    //constructor
    Entity(int x, int y, int entitySize)
    {
        this.x = x;
        this.y = y;
        this.entitySize = entitySize;
    }

    //methods, if any
}